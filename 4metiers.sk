function Check_Miner(p:player):
	set {JOB.Check.Miner} to ({@JOB.Start-xp}+{@JOB.Start-xp}*{JOB.Miner.Level.%{_p}%}^{@JOB.Difficulty})
	if {JOB.Miner.xp.%{_p}%} is greater than {JOB.Check.Miner}-1:
		remove {JOB.Check.Miner} from {JOB.Miner.xp.%{_p}%}
		add 1 to {JOB.Miner.Level.%{_p}%}
		add ({JOB.Miner.Level.%{_p}%} * 1000) to {_p}'s balance
		send "&2&lBien joué &3&l%{_p}% %nl%&2&lTu es passé niveau &9&l%{JOB.Miner.Level.%{_p}%}% &2&ldans le &e&lmétier &2&lde &f&lMineur" to {_p}
	create bossbar titled "&f&lNiveau %{JOB.Miner.Level.%{_p}%}% dans le métier de Mineur" and id "JOB.Miner.BossBar" for {_p} with progress ({JOB.Miner.xp.%{_p}%}/{JOB.Check.Miner}*100) with colors white
	stop

